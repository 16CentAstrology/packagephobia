{
  "name": "packagephobia",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "git-pre-commit": "npm run prettier && git add -A",
    "lint": "npm run prettier && git diff --exit-code",
    "test": "tsc && tape \"dist/test/**/*.js\""
  },
  "repository": "styfle/packagephobia",
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.0",
    "npm": "^6.14.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.0",
    "redis": "^3.0.2",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.36",
    "@types/node-fetch": "^2.5.6",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/redis": "^2.8.18",
    "@types/semver": "^7.1.0",
    "@types/tape": "^4.13.0",
    "@zeit/git-hooks": "^0.1.4",
    "prettier": "^2.0.4",
    "tape": "^5.0.0",
    "typescript": "^3.8.3"
  }
}
