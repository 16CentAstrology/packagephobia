{
  "name": "packagephobia",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "cp -r static public",
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "git-pre-commit": "npm run prettier && git add -A",
    "lint": "npm run prettier && git diff --exit-code",
    "test": "tsc && tape \"dist/test/**/*.js\""
  },
  "repository": "styfle/packagephobia",
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.0",
    "npm": "^6.13.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redis": "^3.0.2",
    "semver": "^7.1.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.27",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/redis": "^2.8.15",
    "@types/semver": "^7.1.0",
    "@types/tape": "^4.2.34",
    "@zeit/git-hooks": "^0.1.4",
    "prettier": "^1.19.1",
    "tape": "^4.13.0",
    "typescript": "^3.7.5"
  }
}
