{
  "name": "packagephobia",
  "version": "1.0.0",
  "description": "Find the cost of adding a new dev dependency to your project",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "prettier": "prettier \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "prettier:check": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "now --public && now alias"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/styfle/packagephobia.git"
  },
  "keywords": [],
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "isomorphic-unfetch": "^2.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.1",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/redis": "^2.8.6",
    "prettier": "^1.11.1",
    "typescript": "^2.8.1"
  }
}
